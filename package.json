{
  "name": "@natlibfi/melinda-rest-api-importer",
  "description": "API microservice of Melinda record batch import system",
  "author": {
    "name": "The National Library of Finland"
  },
  "keywords": [],
  "homepage": "https://github.com/NatLibFi/melinda-rest-api-importer",
  "bugs": {
    "url": "https://github.com/NatLibFi/melinda-rest-api-importer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:NatLibFi/melinda-rest-api-importer.git"
  },
  "license": "AGPL-3.0+",
  "version": "3.0.10",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=18"
  },
  "private": true,
  "scripts": {
    "start": "node ./dist/index",
    "start:dev": "babel-node ./src/index",
    "lint": "eslint ./src",
    "lint:dev": "eslint ./src --fix",
    "test": "npm run lint",
    "dev": "NODE_ENV=development cross-env nodemon",
    "build": "babel src --source-maps --copy-files --delete-dir-on-start --out-dir=dist",
    "prod": "NODE_ENV=production npm run build && npm run start"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.6",
    "@natlibfi/marc-record": "^7.2.4",
    "@natlibfi/marc-record-serializers": "^10.0.5",
    "@natlibfi/melinda-backend-commons": "^2.2.1",
    "@natlibfi/melinda-commons": "^13.0.5",
    "@natlibfi/melinda-rest-api-commons": "^4.0.7",
    "http-status": "^1.6.2",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.12",
    "pretty-print-ms": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/preset-env": "^7.22.9",
    "@babel/register": "^7.22.5",
    "@natlibfi/eslint-config-melinda-backend": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "nodemon": "^3.0.1"
  },
  "eslintConfig": {
    "extends": "@natlibfi/melinda-backend"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "maintained node versions"
        }
      ]
    ]
  },
  "nodemonConfig": {
    "exec": "npm run lint:dev && npm run start:dev",
    "watch": [
      "src/*"
    ],
    "ext": "js",
    "delay": 2.5
  }
}
